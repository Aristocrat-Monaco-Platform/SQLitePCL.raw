<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net5.0;uap10.0</TargetFrameworks>
    <LangVersion>$(ProviderLangVersion)</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\sn\SQLitePCLRaw.provider.e_sqlcipher.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

<ItemGroup>
	<ProjectReference Include="..\SQLitePCLRaw.core\SQLitePCLRaw.core.csproj" />
</ItemGroup>

<ItemGroup Condition=" '$(TargetFramework)' == 'netstandard2.0' ">
    <Compile Include="Generated\netstandard2.0\provider_e_sqlcipher.cs" />
</ItemGroup>

<ItemGroup Condition=" '$(TargetFramework)' == 'net5.0' ">
    <Compile Include="Generated\net5.0\provider_e_sqlcipher.cs" />
</ItemGroup>

<ItemGroup Condition=" '$(TargetFramework)' == 'uap10.0' ">
    <Compile Include="Generated\uap10.0\provider_e_sqlcipher.cs" />
</ItemGroup>

</Project>
